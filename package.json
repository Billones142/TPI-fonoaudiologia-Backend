{
  "module": "src/index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "start": "bun ./src/server.ts",
    "dev": "bun --watch ./src/server.ts",
    "eslint-check": "bunx --bun eslint --ext .ts src",
    "prisma-generate": "bunx --bun prisma generate",
    "build-deploy-executable": "bun install && bun prisma-generate && bun build --compile --minify --sourcemap --bytecode ./src/server.ts --outfile fonoBackEndServer",
    "prisma-dev-migrate": "bunx --bun prisma migrate dev"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/cookie-parser": "^1.4.8",
    "@types/express": "^5.0.1",
    "eslint": "^8.56.0",
    "globals": "^16.2.0",
    "openapi-typescript-codegen": "^0.29.0"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@prisma/client": "^6.8.2",
    "@types/cors": "^2.8.18",
    "@types/crypto-js": "^4.2.2",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "express": "^5.1.0",
    "jest": "^29.7.0",
    "jsonwebtoken": "^9.0.2",
    "prisma": "^6.8.2",
    "supertest": "^7.1.1"
  }
}